{
  "name": "grommet-icons-site",
  "version": "0.1.0",
  "main": "src/js/index.js",
  "description": "website for grommet icons",
  "homepage": "https://grommet.github.io/grommet-icons/",
  "bugs": "https://github.com/grommet/grommet-icons/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/grommet/grommet-icons.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "check": "cross-env NODE_ENV=test grommet check",
    "dev": "cross-env NODE_ENV=development grommet pack",
    "dev-alias": "cross-env NODE_ENV=development USE_ALIAS=true grommet pack",
    "dist": "cross-env NODE_ENV=production grommet pack && npm run generate-html",
    "test": "grommet check -t",
    "test-update": "grommet check -t -- --updateSnapshot",
    "test-watch": "grommet check -t -- --watchAll",
    "generate-html": "cross-env NODE_ENV=ssr babel-node ./tools/generate-html"
  },
  "dependencies": {
    "grommet": "https://github.com/grommet/grommet/tarball/NEXT-stable",
    "grommet-icons": "https://github.com/grommet/grommet-icons/tarball/stable",
    "prop-types": "^15.5.10",
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.5.2",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-grommet": "^0.4.0",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "deep-assign": "^2.0.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-parallel": "^0.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "grommet-cli": "^5.1.0",
    "html-minifier": "^3.5.5",
    "pre-commit": "^1.2.2",
    "react-dev-utils": "^0.4.2",
    "react-test-renderer": "^15.4.1",
    "simple-git": "^1.77.0",
    "webpack": "^3.4.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/templates/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|templates|node_modules)[/\\\\]"
    ]
  },
  "pre-commit": [
    "check"
  ]
}
